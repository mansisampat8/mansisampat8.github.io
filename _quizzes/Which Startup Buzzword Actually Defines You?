  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "Quiz",
    "name": "Which Startup Buzzword Actually Defines You?",
    "url": "https://www.consumerkanvas.com/quizzes/which-buzzword-defines-you/",
    "description": "Quick quiz to reveal which startup buzzword best matches your approach — Growth Hacking, Disruptive, Scalable, Data-Driven, or Customer-Centric.",
    "creator": {
      "@type": "Organization",
      "name": "Consumer Kanvas",
      "url": "https://www.consumerkanvas.com/"
    }
  }
  </script>
</head>
<body>
  <div class="quiz-shell">

    <div class="brand-row">
      <a class="logo-btn" href="/index.html">Consumer Kanvas</a>
      <div class="quiz-title">Which Startup Buzzword Actually Defines You?</div>
    </div>

    <div class="card" id="introCard" style="text-align:center;">
      <h2 style="margin-top:0;">Which Startup Buzzword Actually Defines You?</h2>
      <p style="font-size:1.05rem; line-height:1.45;">
        Buzzwords are shorthand — honest signals of how you think and what you prioritize. Some founders are growth hackers, others obsess over scale or data. This quiz tells you which buzzword actually fits your approach.
      </p>
      <p style="font-size:1.05rem; line-height:1.45; margin-top:12px;">
        You’ll get a single buzzword plus practical next steps: what to double down on and what to fix. Ready? This takes under two minutes.
      </p>
      <button id="startQuiz" class="btn btn-primary" style="margin-top:18px;">Start</button>
    </div>

    <div class="card" id="quizCard" aria-live="polite" style="display:none;">
      <div class="progress-wrap">
        <div class="progress-bar"><div id="progressFill" class="progress-fill"></div></div>
        <div class="progress-text" id="progressText">0 / 7</div>
      </div>

      <div id="questionBlock"></div>

      <div class="nav-row">
        <button id="backBtn" class="btn btn-secondary" style="visibility:hidden">Back</button>
        <div style="flex:1"></div>
        <button id="nextBtn" class="btn btn-primary" disabled>Next</button>
      </div>
    </div>

    <div class="card" id="resultCard" style="display:none">
      <div class="result-card">
        <span id="resultEmoji" class="result-emoji">💬</span>
        <div class="score" id="finalScore">Your Buzzword</div>
        <div class="result-copy" id="finalText"></div>
        <div class="share-row">
          <button class="share-btn" id="waShare">WhatsApp</button>
          <button class="share-btn" id="twShare">Twitter</button>
          <button class="share-btn" id="lnShare">LinkedIn</button>
        </div>
      </div>
    </div>

  </div>

<script>
  /* Quiz content — 7 questions, strategic/practical tone */
  const questions = [
    { text:"When you talk about your startup, you emphasize:", options:[
      {label:"Rapid experiments that unlock quick growth", archetype:"GrowthHacker"},
      {label:"Changing how people do something entirely", archetype:"Disruptive"},
      {label:"Building something that scales without breaking", archetype:"Scalable"},
      {label:"Metrics, tests and repeatable signals", archetype:"DataDriven"},
      {label:"Solving a real customer pain and delighting users", archetype:"CustomerCentric"} ]},

    { text:"Your go-to metric is:", options:[
      {label:"Viral coefficient or activation rate", archetype:"GrowthHacker"},
      {label:"Market share shift or category adoption", archetype:"Disruptive"},
      {label:"Unit economics at scale", archetype:"Scalable"},
      {label:"Cohort retention and LTV/CAC", archetype:"DataDriven"},
      {label:"NPS and repeat purchase rate", archetype:"CustomerCentric"} ]},

    { text:"Your team praises you for:", options:[
      {label:"Creative hacks that move the needle fast", archetype:"GrowthHacker"},
      {label:"Seeing opportunities others dismiss", archetype:"Disruptive"},
      {label:"Designing systems that run cleanly as you grow", archetype:"Scalable"},
      {label:"Turning messy data into decisive action", archetype:"DataDriven"},
      {label:"Knowing customers by name and need", archetype:"CustomerCentric"} ]},

    { text:"In a crisis, you respond by:", options:[
      {label:"Trying a bold experiment immediately", archetype:"GrowthHacker"},
      {label:"Reframing how the product is positioned", archetype:"Disruptive"},
      {label:"Fixing the bottleneck that blocks scale", archetype:"Scalable"},
      {label:"Looking into the data and isolating causes", archetype:"DataDriven"},
      {label:"Talking to customers and solving the pain", archetype:"CustomerCentric"} ]},

    { text:"Your pitch line most often includes:", options:[
      {label:"A quick growth hack we used to beat competitors", archetype:"GrowthHacker"},
      {label:"How we make an old industry irrelevant", archetype:"Disruptive"},
      {label:"How we’ll grow 10x without doubling costs", archetype:"Scalable"},
      {label:"The tests that prove demand", archetype:"DataDriven"},
      {label:"Stories of customers who switched and loved us", archetype:"CustomerCentric"} ]},

    { text:"Where do you invest most effort today?", options:[
      {label:"Creative acquisition experiments", archetype:"GrowthHacker"},
      {label:"Refining the core insight that differentiates us", archetype:"Disruptive"},
      {label:"Automating and removing manual touchpoints", archetype:"Scalable"},
      {label:"Instrumentation and measurement", archetype:"DataDriven"},
      {label:"Customer success and experience", archetype:"CustomerCentric"} ]},

    { text:"If you had to strengthen one thing next, it would be:", options:[
      {label:"More reproducible tactics that scale", archetype:"GrowthHacker"},
      {label:"Stronger defensibility against incumbents", archetype:"Disruptive"},
      {label:"Better systems for predictable growth", archetype:"Scalable"},
      {label:"Cleaner data and faster learning loops", archetype:"DataDriven"},
      {label:"Deeper customer research and product fit", archetype:"CustomerCentric"} ]}
  ];

  const totalQuestions = questions.length;
  let current = 0;
  const answers = new Array(totalQuestions).fill(null);

  const questionBlock = document.getElementById('questionBlock');
  const progressFill = document.getElementById('progressFill');
  const progressText = document.getElementById('progressText');
  const nextBtn = document.getElementById('nextBtn');
  const backBtn = document.getElementById('backBtn');

  function renderQuestion(index){
    const q = questions[index];
    questionBlock.innerHTML = '';

    const qnum = document.createElement('div');
    qnum.className='q-number';
    qnum.textContent=`Question ${index+1} of ${totalQuestions}`;
    questionBlock.appendChild(qnum);

    const qtext = document.createElement('div');
    qtext.className='q-text';
    qtext.textContent=q.text;
    questionBlock.appendChild(qtext);

    const opts=document.createElement('div'); opts.className='options';
    q.options.forEach(opt=>{
      const b=document.createElement('button');
      b.type='button'; b.className='opt'; b.textContent=opt.label;
      b.onclick=()=>{
        answers[index]=opt.archetype;
        opts.querySelectorAll('.opt').forEach(x=>x.classList.remove('selected'));
        b.classList.add('selected');
        nextBtn.disabled=false;
      };
      if(answers[index]===opt.archetype) b.classList.add('selected');
      opts.appendChild(b);
    });
    questionBlock.appendChild(opts);

    backBtn.style.visibility=index===0?'hidden':'visible';
    nextBtn.textContent=index===totalQuestions-1?'See Result':'Next';
    nextBtn.disabled=answers[index]===null;
    updateProgress();
  }

  function updateProgress(){
    const pct = Math.round(((current+1)/totalQuestions)*100);
    progressFill.style.width = pct + '%';
    progressText.textContent = `${current+1} / ${totalQuestions}`;
  }

  nextBtn.addEventListener('click',()=>{
    if(current<totalQuestions-1){
      current++;
      renderQuestion(current);
    } else {
      if(answers.includes(null)){ alert("Please answer all questions before viewing results."); return; }
      showResult();
    }
  });

  backBtn.addEventListener('click',()=>{ if(current>0){ current--; renderQuestion(current); } });

  function showResult(){
    const tally={GrowthHacker:0,Disruptive:0,Scalable:0,DataDriven:0,CustomerCentric:0};
    answers.forEach(a=>{ if(a && tally.hasOwnProperty(a)) tally[a]++; });

    // winner with tie-breaker based on earliest appearance
    const winner = Object.keys(tally).reduce((best, key) => {
      if(tally[key] > (tally[best] || 0)) return key;
      if(tally[key] === (tally[best] || 0)) {
        const firstIndexKey = answers.findIndex(a => a===key);
        const firstIndexBest = answers.findIndex(a => a===best);
        return (firstIndexKey !== -1 && firstIndexBest !== -1) ? (firstIndexKey < firstIndexBest ? key : best) : best;
      }
      return best;
    }, Object.keys(tally)[0]);

    let title="", text="", emoji="💬", nextSteps="";

    switch(winner){
      case "GrowthHacker":
        title = "Growth Hacking";
        emoji = "⚡";
        text = "You prioritise high-impact experiments and creative acquisition tactics. You move fast, test cheap, and scale winners quickly.";
        nextSteps = "Double down on repeatable funnels: document successful hacks, turn them into playbooks, and invest in small automation to scale the tactics.";
        break;
      case "Disruptive":
        title = "Disruptive";
        emoji = "🚧";
        text = "You aim to rewrite category rules. Your strength is big insight and reframing problems others accept as fixed.";
        nextSteps = "Sharpen your defensibility: map incumbents, clarify why customers will change behaviour, and outline the sequence of moves to win adoption.";
        break;
      case "Scalable":
        title = "Scalable";
        emoji = "📈";
        text = "You think systems first. You design for growth without adding proportional cost — predictable scaling is your north star.";
        nextSteps = "Invest in reliability: automate manual work, standardise onboarding, and measure unit economics across growth scenarios.";
        break;
      case "DataDriven":
        title = "Data-Driven";
        emoji = "📊";
        text = "You make decisions from signals. Tests, cohorts, and clear metrics guide you — you trust what you can measure and improve.";
        nextSteps = "Clean your instrumentation, prioritise A/B tests with clear hypotheses, and build dashboards that focus the team on learning velocity.";
        break;
      case "CustomerCentric":
        title = "Customer-Centric";
        emoji = "❤️";
        text = "You centre the product around real human needs. You win loyal users who become advocates because you obsess over experience.";
        nextSteps = "Scale your customer voice: systematise feedback loops, prioritise feature decisions by customer value, and measure experience metrics alongside growth.";
        break;
      default:
        title = "Hybrid Approach";
        text = "You show a mix of approaches. Choose one axis to sharpen first — metrics, narrative, or systems — then align the team."
        nextSteps = "Pick the most actionable next step from the guidance above and run a 30-day plan to show measurable improvement.";
    }

    document.getElementById('quizCard').style.display='none';
    document.getElementById('resultCard').style.display='block';
    document.getElementById('finalScore').textContent = title;
    document.getElementById('finalText').innerHTML = `<strong>${text}</strong><br><br>${nextSteps}`;
    document.getElementById('resultEmoji').textContent = emoji;
  }

  document.addEventListener("DOMContentLoaded", function() {
    const startBtn = document.getElementById('startQuiz');
    const introCard = document.getElementById('introCard');
    const quizCardLocal = document.getElementById('quizCard');

    startBtn.addEventListener('click', function() {
      introCard.style.display = 'none';
      quizCardLocal.style.display = 'block';
      renderQuestion(0);
    });

    document.addEventListener('keydown', function(e){
      if(document.activeElement && document.activeElement.classList && document.activeElement.classList.contains('opt') && (e.key === 'Enter' || e.key === ' ')){
        document.activeElement.click();
      }
    });
  });

  // === SOCIAL SHARE BUTTONS ===
  const pageUrl = encodeURIComponent("https://www.consumerkanvas.com/quizzes/which-buzzword-defines-you/");
  document.getElementById("waShare").addEventListener("click", function () {
    const text = encodeURIComponent("Which startup buzzword defines you? I just took this quick quiz on Consumer Kanvas — try it.");
    window.open(`https://wa.me/?text=${text}%20${pageUrl}`, "_blank");
  });

  document.getElementById("twShare").addEventListener("click", function () {
    const text = encodeURIComponent("Which startup buzzword defines you? I just found mine — take the quiz.");
    window.open(`https://twitter.com/intent/tweet?text=${text}&url=${pageUrl}`, "_blank");
  });

  document.getElementById("lnShare").addEventListener("click", function () {
    const title = encodeURIComponent("Which Startup Buzzword Actually Defines You?");
    const summary = encodeURIComponent("Quick quiz by Consumer Kanvas that reveals which startup buzzword matches your approach — and practical next steps.");
    window.open(`https://www.linkedin.com/sharing/share-offsite/?url=${pageUrl}&title=${title}&summary=${summary}`, "_blank");
  });
</script>
</body>
</html>
